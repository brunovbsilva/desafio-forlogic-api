{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:30013"
        }
      }
    ]
  },
  "AppSettings": {
    "Jwt": {
      "Secret": "A1B2C3D4A5B6C7D8A1B2C3D4A5B6C7D8A1B2C3D4A5B6C7D8",
      "SessionExpirationHours": 5
    }
  },
    "ConnectionStrings": {
        "Postgres": "Host=postgres;Port=5432;Database=postgres;Username=admin;Password=password",
        "Redis": "redis:6379"
    }
}
